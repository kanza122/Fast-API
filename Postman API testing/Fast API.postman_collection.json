		{
			"name": "Login request (Auth 2.0)",
			"event": [
				{
					"listen": "test",
					"script": {
						"exec": [
							"//Test the status code is 200\r",
							"pm.test(\"Status code is 200\", function () {\r",
							"    pm.response.to.have.status(200);\r",
							"});\r",
							"//Check if response has token\r",
							"pm.test(\"Response has token\", function (){\r",
							"    let value=pm.response.json();\r",
							"    pm.expect(value).to.have.property(\"access_token\");\r",
							"})\r",
							"// Insure password or other private data is not returned\r",
							"pm.test(\"Response does not contain private data\", function () {\r",
							"    let value = pm.response.json();\r",
							"    pm.expect(value).to.not.have.property(\"password\");\r",
							"    \r",
							"})\r",
							"\r",
							"// Set the access token as collection vairable to used in next request\r",
							"pm.test(\"set access token as collection vairable\", function(){\r",
							"    let value= pm.response.json();\r",
							"    if (value && value.access_token) {\r",
							"    pm.collectionVariables.set(\"token\",value.access_token)\r",
							"    console.log(\"token is valid\")}\r",
							"    else{\r",
							"        console.log(\"token is invalid\")\r",
							"    }\r",
							"})\r",
							"pm.test(\"set email value in collection \",function(){\r",
							"    let value=pm.response.json()\r",
							"    pm.collectionVariables.set(\"email\",value.email)\r",
							"})\r",
							"//Check the response time (performance testing)\r",
							"pm.test(\"Response time is less than 1000ms\", function () {\r",
							"    pm.expect(pm.response.responseTime).to.be.below(1000);\r",
							"});\r",
							""
						],
						"type": "text/javascript",
						"packages": {},
						"requests": {}
					}
				}
			],
